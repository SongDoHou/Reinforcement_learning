    "import time\n",
    "\n",
    "def play_render(agent,env,time_sleep=0.2):\n",
    "    '''\n",
    "    학습 이후 에이전트의 플레이 화면을 보여줍니다.\n",
    "    입력변수\n",
    "        agent: 에이전트\n",
    "        env: 환경\n",
    "        time_sleep: 플레이 화면표시 주기. 기본 0.2초 간격으로 시점변화\n",
    "    '''\n",
    "    s = env.reset()\n",
    "    cnt = 0\n",
    "    while True:\n",
    "        time.sleep(time_sleep)\n",
    "        clear_output(True)\n",
    "        print(f'Step: {cnt}')\n",
    "        env.render()\n",
    "        action = agent.epsilon_action(s)\n",
    "        next_s, reward, done, _ = env.step(action)\n",
    "        if done:\n",
    "            clear_output(True)\n",
    "            print(f'Step: {cnt}')\n",
    "            env.render()\n",
    "            break\n",
    "        s = next_s\n",
    "        cnt+=1"
   ]
